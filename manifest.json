{
  "manifest_version": 2,
  "name": "X-Frame Options Remover",
  "version": "1.0",
  "description": "A Chrome extension to remove X-Frame-Options headers.",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "page": "background.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}


